eturn TruckIcon;\n        case 'assign':\n          return PlusIcon;\n        default:\n          return PlusIcon;\n      }\n    };\n    const getActivityColor = type => {\n      switch (type) {\n        case 'user':\n          return 'bg-blue-500';\n        case 'vehicle':\n          return 'bg-green-500';\n        case 'assign':\n          return 'bg-purple-500';\n        default:\n          return 'bg-gray-500';\n      }\n    };\n    const formatDate = date => {\n      return format(new Date(date), 'dd MMM yyyy HH:mm', {\n        locale: es\n      });\n    };\n    onMounted(() => {\n      fetchData();\n    });\n    const __returned__ = {\n      toast,\n      roles,\n      marcas,\n      tiposVehiculos,\n      usuariosDisponibles,\n      vehiculosDisponibles,\n      recentActivity,\n      userLoading,\n      vehicleLoading,\n      assignLoading,\n      userForm,\n      vehicleForm,\n      assignForm,\n      fetchData,\n      registerUser,\n      registerVehicle,\n      quickAssign,\n      getActivityIcon,\n      getActivityColor,\n      formatDate,\n      ref,\n      onMounted,\n      get PlusIcon() {\n        return PlusIcon;\n      },\n      get TruckIcon() {\n        return TruckIcon;\n      },\n      get UserPlusIcon() {\n        return UserPlusIcon;\n      },\n      get api() {\n        return api;\n      },\n      get useToast() {\n        return useToast;\n      },\n      get format() {\n        return format;\n      },\n      get es() {\n        return es;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","PlusIcon","TruckIcon","UserPlusIcon","api","useToast","format","es","toast","roles","marcas","tiposVehiculos","usuariosDisponibles","vehiculosDisponibles","recentActivity","userLoading","vehicleLoading","assignLoading","userForm","nombre","apellido","email","telefono","password","rol_id","veh